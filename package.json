{
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "yarn clean && tsc",
    "lint": "eslint \"src\"",
    "start": "npx dotenv-cli -c '.env' -- node dist/app.js",
