    "cloudinary": "^2.8.0",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "pg": "latest"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
